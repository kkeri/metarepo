	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "attach",
			"name": "Attach",
			"port": 9229,
			"smartStep": true,
			"outFiles": [
				"${workspaceFolder}/lib/**/*.js"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Program",
			"smartStep": true,
			"program": "${workspaceFolder}\\lib\\index.js",
			"preLaunchTask": "tsc: build - tsconfig.json",
			"outFiles": [
				"${workspaceFolder}/lib/**/*.js"
			]
		}
	]
}